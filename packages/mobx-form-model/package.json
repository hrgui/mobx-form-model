{
  "name": "@hrgui/mobx-form-model",
  "version": "0.6.1-0",
  "main": "dist/index.js",
  "license": "MIT",
  "module": "src/index.ts",
  "scripts": {
    "test": "jest",
    "docs": "typedoc --out docs/ --theme markdown --exclude '**/*.spec.+(ts|tsx)' --readme none src/",
    "build": "tsc"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.0",
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^20.0.5",
    "@types/lodash": "^4.14.108",
    "@types/node": "^8.0.10",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/yup": "^0.24.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-mobx": "^1.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "jest": "^22.4.2",
    "jest-enzyme": "^4.2.0",
    "mobx": "^4.2.1",
    "mobx-react": "^5.1.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-test-renderer": "^16.3.1",
    "react-testing-library": "^2.1.1",
    "ts-jest": "^22.4.6",
    "typedoc": "^0.11.1",
    "typedoc-plugin-markdown": "^1.1.8",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "filter-react-props": "^0.0.4",
    "lodash": "^4.17.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupTestFrameworkScriptFile": "./test/setup.ts",
    "setupFiles": [
      "./test/polyfill.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "test/tsconfig.test.json"
      }
    },
    "testURL": "http://localhost",
    "testEnvironment": "jsdom",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mocks/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testMatch": [
      "<rootDir>/src/**/*.spec.(ts|tsx)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.ts",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageDirectory": "<rootDir>/coverage"
  }
}
